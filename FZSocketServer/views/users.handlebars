

<p>用户管理</p>


<div class="weui-cells__title">带说明的列表项</div>
<div class="weui-cells">
    {{#each people}}
        <div class="weui-cell">
            <div class="weui-cell__bd">
                <p>用户名</p>
            </div>
            <div class="weui-cell__ft">{{firstName}} . {{lastName}}</div>
        </div>
    {{/each}}

</div>

<div class="weui-panel weui-panel_access">
    <div class="weui-panel__hd">用户列表</div>
    <div class="weui-panel__bd">
        {{#each people}}
            <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                <div class="weui-media-box__hd">
                    <img class="weui-media-box__thumb" src="{{avatarUrl}}" alt="">
                </div>
                <div class="weui-media-box__bd">
                    <h4 class="weui-media-box__title">用户名</h4>
                    <p class="weui-media-box__desc">{{firstName}} . {{lastName}}</p>
                </div>
            </a>
        {{/each}}



    </div>
    <div class="weui-panel__ft">
        <a href="javascript:void(0);" class="weui-cell weui-cell_access weui-cell_link">
            <div class="weui-cell__bd">查看更多</div>
            <span class="weui-cell__ft"></span>
        </a>
    </div>

<ol>
{{#each people}}
    <li>{{firstName}} . {{lastName}}</li>
{{/each}}
</ol>


